package interviewlogic;
import java.util.*;
public class h_sumofnumber {
    public static void main(String[] args) {
        Scanner DJ = new Scanner(System.in);
        int n = DJ.nextInt();
        int sum =0;
        int temp=n;
        while(n!=0){
            int digit = n%10;
            sum+=digit;
            n/=10;
        }
        System.out.println(sum);
    }
}
